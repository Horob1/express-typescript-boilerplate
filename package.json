{
  "name": "express-typescript-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for express typescript",
  "keywords": [
    "boilerplate",
    "express",
    "typescript"
  ],
  "license": "MIT",
  "author": "ACN Mercenary",
  "main": "./src/index.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "dev": "cross-env NODE_ENV=dev nodemon --exec ts-node -r tsconfig-paths/register ./src/index.ts",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "dev:cron": "cross-env NODE_ENV=dev nodemon --exec ts-node -r tsconfig-paths/register ./src/worker/cron/index.ts",
    "start:cron": "cross-env NODE_ENV=production node ./dist/worker/cron/index.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/ms": "^2.1.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/pug": "^2.0.10",
    "@types/streamifier": "^0.1.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "dependencies": {
    "@getbrevo/brevo": "^2.1.1",
    "@noble/hashes": "^1.7.2",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "cloudinary": "^2.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "exit-hook": "^4.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "pug": "^3.0.3",
    "redis": "^4.7.0",
    "socket.io": "^4.8.1",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "ua-parser-js": "^2.0.3",
